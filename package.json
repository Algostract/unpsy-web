{
  "name": "unpsy-web",
  "version": "0.6.2",
  "description": "A psychoanalytical web tool for diverse assessments and automatic analysis with a built-in scanner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/unpsy-web.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.14.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0"
  },
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "clean": "nuxt cleanup",
    "start": "node .output/server/index.mjs",
    "db:seed": "prisma db seed",
    "db:backup": "npx tsx prisma/backup.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION=dev --build-arg BUILD_TIME=now -t unpsy-web:dev .",
    "docker:start": "docker run --detach --name unpsy-web-dev --env-file .env.prod -p 4000:3000 unpsy-web:dev",
    "tunnel": "untun tunnel --port 4000 http://localhost:4000"
  },
  "prisma": {
    "seed": "jiti prisma/scripts/seed.ts"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.16",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/scripts": "^0.10.4",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^2.2.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.10.1",
    "@prisma/client": "^6.4.1",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "@splidejs/vue-splide": "^0.6.12",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/core": "^12.8.2",
    "@vueuse/nuxt": "^12.8.2",
    "eslint": "^9.22.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.15.4",
    "nuxt-icons": "^3.2.1",
    "pinia": "^3.0.1",
    "prisma": "^6.4.1",
    "uqr": "^0.1.2",
    "vee-validate": "^4.15.0",
    "vue": "latest",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.3",
    "untun": "^0.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}