{
  "name": "unpsy-web",
  "version": "0.7.8",
  "description": "A psychoanalytical web tool for diverse assessments and automatic analysis with a built-in scanner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algostract/unpsy-web.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.16",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "clean": "nuxt cleanup",
    "start": "node .output/server/index.mjs",
    "docker:build": "docker build --build-arg VERSION=dev --build-arg BUILD_TIME=now -t unpsy:dev .",
    "docker:start": "docker run --detach --name unpsy-dev --env-file .env.prod -p 4000:3000 unpsy:dev",
    "tunnel": "untun tunnel --port 4000 http://localhost:4000"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.2",
    "@notionhq/client": "^3.1.3",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.13.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "0.11.5",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.5.5",
    "@nuxtjs/seo": "^3.0.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.1",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "~2.2.7",
    "@unhead/vue": "^2.0.10",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.3.0",
    "@vueuse/nuxt": "^13.3.0",
    "nuxt": "^3.17.5",
    "nuxt-auth-utils": "^0.5.20",
    "nuxt-splide": "1.1.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "web-push": "^3.6.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tauri-apps/cli": "^2.5.0",
    "@types/k6": "^1.0.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.29.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "playwright-core": "^1.53.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "typescript": "^5.8.3",
    "untun": "^0.1.3",
    "vitest": "^3.2.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "protobufjs",
    "sharp",
    "unrs-resolver"
  ]
}
