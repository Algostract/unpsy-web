{
  "name": "unpsy",
  "description": "A psychoanalytical web tool for diverse assessments and automatic analysis with a built-in scanner",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "version": "0.6.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.15.0"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier .  --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "tunnel": "untun tunnel --port 3000  http://localhost:3000",
    "clean": "nuxt cleanup",
    "db:start": "cockroach start-single-node --insecure --listen-addr=localhost --store=database",
    "db:seed": "prisma db seed",
    "db:backup": "npx tsx prisma/scripts/backup.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION_TAG=dev -t unpsy:dev .",
    "docker:start": "docker run --detach --name unpsy --env-file .env.prod -p 2002:3000 ghcr.io/shba007/unpsy:latest"
  },
  "prisma": {
    "seed": "npx tsx prisma/scripts/seed.ts"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@prisma/client": "^5.17.0",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "@splidejs/vue-splide": "^0.6.12",
    "jsonwebtoken": "^9.0.2",
    "uqr": "^0.1.2",
    "vee-validate": "^4.13.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@nuxt/eslint": "^0.4.0",
    "@nuxt/fonts": "^0.7.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.14.0",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/seo": "^2.0.0-rc.16",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "@vite-pwa/nuxt": "^0.9.1",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "eslint-config-prettier": "^9.1.0",
    "nuxt": "^3.12.4",
    "nuxt-gtag": "^2.1.0",
    "nuxt-icons": "^3.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "untun": "^0.1.3"
  }
}